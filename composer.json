{
    "name": "zeichen32/gitlabapibundle",
    "description": "Symfony Bundle to include the gitlab api.",
    "type": "symfony-bundle",
    "keywords": ["gitlab", "api"],
    "homepage": "https://github.com/Zeichen32/GitLabApiBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Jens Averkamp",
            "email": "j.averkamp@two-developers.com"
        }
    ],
    "require": {
        "php": "^7.2",
        "symfony/framework-bundle": "^4.4|^5.0",
        "symfony/http-client": "^4.4|^5.0",
        "nyholm/psr7": "^1.3",
        "m4tthumphrey/php-gitlab-api": "^10.0"
    },
    "require-dev" : {
        "php-http/message": "^1.6",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-0": { "Zeichen32\\GitLabApiBundle": "" }
    },
    "target-dir": "Zeichen32/GitLabApiBundle",
    "extra": {
        "branch-alias": {
            "dev-master": "4.0.x-dev"
        }
    }
}
